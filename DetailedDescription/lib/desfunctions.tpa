		PATCH_IF (readtype == 0) && (readtimemode == 2) BEGIN SPRINT newdesc ~%newdesc%~^~的豁免修正：%readamount%~ SET addnum = pnum + 1 END
		PATCH_IF (readtype == 1) && (readtimemode == 2) BEGIN SPRINT newdesc ~%newdesc%~^~的豁免设为：%readamount%~ SET addnum = pnum + 1 END
		PATCH_IF (readtype == 2) && (readtimemode == 2) BEGIN SPRINT newdesc ~%newdesc%~^~的豁免设为当前的%readamount%％~ SET addnum = pnum + 1 END
	END


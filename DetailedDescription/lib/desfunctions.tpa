		PATCH_IF (readtype == 7) && (readtimemode == 2) BEGIN SPRINT newdesc ~%newdesc%~^~%WNL%挥砍伤害修正：%readamount%％~ SET addnum = pnum + 1 END
		PATCH_IF (readtype == 8) && (readtimemode == 2) BEGIN SPRINT newdesc ~%newdesc%~^~%WNL%穿刺伤害修正：%readamount%％~ SET addnum = pnum + 1 END
		PATCH_IF (readtype == 9) && (readtimemode == 2) BEGIN SPRINT newdesc ~%newdesc%~^~%WNL%钝击伤害修正：%readamount%％~ SET addnum = pnum + 1 END
		PATCH_IF (readtype == 10) && (readtimemode == 2) BEGIN SPRINT newdesc ~%newdesc%~^~%WNL%远程伤害修正：%readamount%％~ SET addnum = pnum + 1 END
	END
	
	PATCH_IF opcode == 346 BEGIN
		READ_LONG (4 + effoffset + 0x30 * (peffect - 1)) readamount
		READ_LONG (8 + effoffset + 0x30 * (peffect - 1)) readtype
		READ_BYTE (12 + effoffset + 0x30 * (peffect - 1)) readtimemode
		READ_LONG (44 + effoffset + 0x30 * (peffect - 1)) readspecial
		PATCH_IF (readspecial == 0) && (readtimemode == 2) BEGIN SET readtype = 3 END
		PATCH_IF (readspecial == 1) && (readtimemode == 2) BEGIN SPRINT newdesc ~%newdesc%~^~%WNL%对防护系~ END

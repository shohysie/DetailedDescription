DEFINE_PATCH_FUNCTION CHECK_EQUIP_EFFECT
INT_VAR
	peffect = 1
	pnum = 0
STR_VAR
	pdesc = ~~
RET
	newdesc
	addnum
BEGIN
	SET addnum = pnum
	SPRINT newdesc ~%pdesc%~
	READ_LONG 0x6a effoffset
	READ_SHORT (effoffset + 0x30 * (peffect - 1)) opcode
	
	PATCH_IF opcode == 0 BEGIN
		READ_LONG (4 + effoffset + 0x30 * (peffect - 1)) readamount
		READ_LONG (8 + effoffset + 0x30 * (peffect - 1)) readtype
		READ_BYTE (12 + effoffset + 0x30 * (peffect - 1)) readtimemode		
		PATCH_IF (readtype == 16) && (readtimemode == 2) BEGIN SPRINT newdesc ~%newdesc%~^~%WNL%防御等级设为：%readamount%~ SET addnum = pnum + 1 END
		PATCH_IF (readtype == 8) && (readtimemode == 2) BEGIN SPRINT newdesc ~%newdesc%~^~%WNL%挥砍防御修正：%readamount%~ SET addnum = pnum + 1 END
		PATCH_IF (readtype == 4) && (readtimemode == 2) BEGIN SPRINT newdesc ~%newdesc%~^~%WNL%穿刺防御修正：%readamount%~ SET addnum = pnum + 1 END
		PATCH_IF (readtype == 2) && (readtimemode == 2) BEGIN SPRINT newdesc ~%newdesc%~^~%WNL%远程防御修正：%readamount%~ SET addnum = pnum + 1 END
		PATCH_IF (readtype == 1) && (readtimemode == 2) BEGIN SPRINT newdesc ~%newdesc%~^~%WNL%钝击防御修正：%readamount%~ SET addnum = pnum + 1 END
		PATCH_IF (readtype == 0) && (readtimemode == 2) BEGIN SPRINT newdesc ~%newdesc%~^~%WNL%防御等级修正：%readamount%~ SET addnum = pnum + 1 END
		//PATCH_PRINT ~readamount = %readamount% readtype = %readtype% readtimemode = %readtimemode% newdesc = %newdesc%~
	END

	PATCH_IF opcode == 1 BEGIN
		READ_LONG (4 + effoffset + 0x30 * (peffect - 1)) readamount
		READ_LONG (8 + effoffset + 0x30 * (peffect - 1)) readtype
		READ_BYTE (12 + effoffset + 0x30 * (peffect - 1)) readtimemode
		PATCH_IF readamount > 5 BEGIN
			SET readamount = (readamount * 2 - 11)
			PATCH_IF (readtype == 0) && (readtimemode == 2) BEGIN SPRINT newdesc ~%newdesc%~^~%WNL%每轮攻击次数修正：%readamount%／2~ SET addnum = pnum + 1 END
			PATCH_IF (readtype == 1) && (readtimemode == 2) BEGIN SPRINT newdesc ~%newdesc%~^~%WNL%每轮攻击次数设为：%readamount%／2~ SET addnum = pnum + 1 END
			PATCH_IF (readtype == 3) && (readtimemode == 2) BEGIN SPRINT newdesc ~%newdesc%~^~%WNL%每轮攻击次数最终设为：%readamount%／2~ SET addnum = pnum + 1 END
		END ELSE BEGIN
			PATCH_IF readamount < "-5" BEGIN
				SET readamount = (readamount * 2 + 11)
				PATCH_IF (readtype == 0) && (readtimemode == 2) BEGIN SPRINT newdesc ~%newdesc%~^~%WNL%每轮攻击次数修正：-%readamount%~ SET addnum = pnum + 1 END
			END ELSE BEGIN
				PATCH_IF (readtype == 0) && (readtimemode == 2) BEGIN SPRINT newdesc ~%newdesc%~^~%WNL%每轮攻击次数修正：%readamount%~ SET addnum = pnum + 1 END
				PATCH_IF (readtype == 1) && (readtimemode == 2) BEGIN SPRINT newdesc ~%newdesc%~^~%WNL%每轮攻击次数设为：%readamount%~ SET addnum = pnum + 1 END
				PATCH_IF (readtype == 2) && (readtimemode == 2) BEGIN SPRINT newdesc ~%newdesc%~^~%WNL%每轮攻击次数设为当前的%readamount%％~ SET addnum = pnum + 1 END
				PATCH_IF (readtype == 3) && (readtimemode == 2) BEGIN SPRINT newdesc ~%newdesc%~^~%WNL%每轮攻击次数最终设为：%readamount%~ SET addnum = pnum + 1 END
			END
		END
	END

	PATCH_IF opcode == 6 BEGIN
		READ_LONG (4 + effoffset + 0x30 * (peffect - 1)) readamount
		READ_LONG (8 + effoffset + 0x30 * (peffect - 1)) readtype
		READ_BYTE (12 + effoffset + 0x30 * (peffect - 1)) readtimemode
		PATCH_IF (readtype == 0) && (readtimemode == 2) BEGIN SPRINT newdesc ~%newdesc%~^~%WNL%魅力修正：%readamount%~ SET addnum = pnum + 1 END
		PATCH_IF (readtype == 1) && (readtimemode == 2) BEGIN SPRINT newdesc ~%newdesc%~^~%WNL%魅力设为：%readamount%~ SET addnum = pnum + 1 END
		PATCH_IF (readtype == 2) && (readtimemode == 2) BEGIN SPRINT newdesc ~%newdesc%~^~%WNL%魅力设为：当前的%readamount%％~ SET addnum = pnum + 1 END
	END

	PATCH_IF opcode == 10 BEGIN
		READ_LONG (4 + effoffset + 0x30 * (peffect - 1)) readamount
